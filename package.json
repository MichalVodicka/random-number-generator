{
  "name": "random-number-generator-umbrella-script",
  "version": "0.0.1",
  "description": "Random number generator - umbrella script",
  "main": "",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "cd be && npm run start-local",
    "start-frontend": "cd fe && npm run start-local",
    "install": "npm run install-backend && npm run install-frontend ",
    "install-backend": "cd be && npm install",
    "install-frontend": "cd fe && npm install"
  },
  "author": "Michal Vodicka",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}

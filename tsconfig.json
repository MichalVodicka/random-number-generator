{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es2015",
    "strict": false,
    "jsx": "react",
    "allowJs": true,
    "declaration": true,
    "lib": ["es2015", "dom"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
